<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="EscritorioModeloPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>teste.ModeloTbProfessor</class>
    <class>teste.ModeloTbRelatorio</class>
    <class>teste.ModeloTbAdvogado</class>
    <class>teste.ModeloTbGrupo</class>
    <class>teste.ModeloTbAgenda</class>
    <class>teste.ModeloTbAnexo</class>
    <class>teste.ModeloTbAluno</class>
    <class>teste.ModeloTbProcesso</class>
    <class>teste.ModeloTbUsuario</class>
    <class>teste.ModeloTbCliente</class>
    <class>teste.ModeloTbVara</class>
    <class>teste.ModeloTbPessoa</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/modelo?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
